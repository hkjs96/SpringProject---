<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xml="http://www.w3.org/XML/1998/namespace"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd ">
	<tlib-version>1.0</tlib-version>
	<short-name>ju</short-name>
	<uri>http://www.anyapart.or.kr/jsonUtils</uri>

	<!-- 이경륜: 자바오브젝트 -> json으로 변환하여 스크립트에서 쓸 수 있음 -->
	<function>
		<name>toJson</name>
		<function-class>kr.or.anyapart.commons.utils.JsonUtils</function-class>
		<function-signature>String toJson(java.lang.Object)</function-signature>
		<example>
  			${ju:toJson(object) }
  		</example>
	</function>
</taglib>
